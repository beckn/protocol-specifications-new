{
  "@context": {
    "@version": 1.1,
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "ev": "./vocab/ev#",
    
    /* ---- Core class mappings ---- */
    "ChargingService": "ev:ChargingService",
    "ChargingOffer": "ev:ChargingOffer",
    "ChargingSession": "ev:ChargingSession",
    "ChargingProvider": "ev:ChargingProvider",

    /* ---- Shared schema.org properties ---- */
    "name": "schema:name",
    "description": "schema:description",
    "price": "schema:price",
    "priceCurrency": "schema:priceCurrency",
    "startTime": "schema:startTime",
    "endTime": "schema:endTime",
    "orderStatus": "schema:orderStatus",
    "serviceLocation": "schema:serviceLocation",
    "geo": "schema:geo",
    "latitude": "schema:latitude",
    "longitude": "schema:longitude",
    "paymentAccepted": "schema:paymentAccepted",
    "image": "schema:image",
    "url": "schema:url",
    "sameAs": "schema:sameAs",
    "identifier": "schema:identifier",
    "areaServed": "schema:areaServed",
    "category": "schema:category",

    /* ---- EV-specific extensions (local ./ev#) ---- */
    "connectorId": "ev:connectorId",
    "connectorType": "ev:connectorType",
    "powerType": "ev:powerType",
    "connectorFormat": "ev:connectorFormat",
    "chargingSpeed": "ev:chargingSpeed",
    "stationStatus": "ev:stationStatus",
    "idleFeePolicy": "ev:idleFeePolicy",

    "authorizationMode": "ev:authorizationMode",
    "authorizationOtpHint": "ev:authorizationOtpHint",

    "trackingId": "ev:trackingId",
    "trackingUrl": "ev:trackingUrl",
    "trackingStatus": "ev:trackingStatus",

    "kwh": "ev:kwh",
    "chargingPeriods": "ev:chargingPeriods",
    "dimensions": "ev:dimensions",
    "dimensionType": "ev:dimensionType",
    "volume": "ev:volume",
    "totalCost": "ev:totalCost",
    "exclVat": "ev:exclVat",
    "inclVat": "ev:inclVat",
    "lastUpdated": "schema:dateModified",

    "vehicleMake": "ev:vehicleMake",
    "vehicleModel": "ev:vehicleModel",
    "sessionStatus": "ev:sessionStatus",

    "buyerFinderFee": "ev:buyerFinderFee",
    "feeType": "ev:feeType",
    "feeValue": "ev:feeValue",

    "reservationId": "ev:reservationId",
    "gracePeriodMinutes": "ev:gracePeriodMinutes",
    "ratingOverall": "ev:ratingOverall",
    "ratingAmenities": "ev:ratingAmenities",    
  },

  "@graph": [
    { "@id": "evp:ChargingService", "owl:equivalentClass": "schema:Service" },
    { "@id": "evp:ChargingOffer", "owl:equivalentClass": "schema:Offer" },
    { "@id": "evp:ChargingSession", "owl:equivalentClass": "schema:Order" },
    { "@id": "evp:ChargingProvider", "owl:equivalentClass": "schema:Organization" }
  ]
}