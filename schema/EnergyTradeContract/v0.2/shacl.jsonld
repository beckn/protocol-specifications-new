{
  "@context": {
    "@version": 1.1,
    "sh": "http://www.w3.org/ns/shacl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vocab": "./vocab.jsonld/"
  },
  "@graph": [
    {
      "@id": "beckn:EnergyTradeContract",
      "@type": "sh:NodeShape",
      "sh:targetClass": "beckn:EnergyTradeContract",
      "sh:name": "EnergyTradeContract",
      "sh:property": [
        {
          "sh:path": "beckn:contractStatus",
          "sh:name": "contractStatus",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "PENDING",
            "ACTIVE",
            "COMPLETED",
            "TERMINATED"
          ]
        },
        {
          "sh:path": "ieee:mRID",
          "sh:name": "sourceMeterId",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "ieee:mRID",
          "sh:name": "targetMeterId",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "beckn:inverterId",
          "sh:name": "inverterId",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "beckn:contractedQuantity",
          "sh:name": "contractedQuantity",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 0
        },
        {
          "sh:path": "./vocab.jsonld/tradeStartTime",
          "sh:name": "tradeStartTime",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        },
        {
          "sh:path": "./vocab.jsonld/tradeEndTime",
          "sh:name": "tradeEndTime",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        },
        {
          "sh:path": "beckn:sourceType",
          "sh:name": "sourceType",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "SOLAR",
            "BATTERY",
            "GRID",
            "HYBRID",
            "RENEWABLE"
          ]
        },
        {
          "sh:path": "beckn:certification",
          "sh:name": "certification",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:settlementCycles",
          "sh:name": "settlementCycles",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:billingCycles",
          "sh:name": "billingCycles",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:wheelingCharges",
          "sh:name": "wheelingCharges",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "schema:dateModified",
          "sh:name": "lastUpdated",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        }
      ]
    }
  ]
}
