      "schema:hasEnumerationMember": [
        { "@id": "beckn:SettlementCycleStatusPending" },
        { "@id": "beckn:SettlementCycleStatusSettled" },
        { "@id": "beckn:SettlementCycleStatusFailed" }
      ]
    },
    {
      "@id": "beckn:SettlementCycleStatusPending",
      "@type": ["beckn:SettlementCycleStatus", "schema:Enumeration"],
      "schema:name": "PENDING",
      "schema:identifier": "PENDING"
    },
    {
      "@id": "beckn:SettlementCycleStatusSettled",
      "@type": ["beckn:SettlementCycleStatus", "schema:Enumeration"],
      "schema:name": "SETTLED",
      "schema:identifier": "SETTLED"
    },
    {
      "@id": "beckn:SettlementCycleStatusFailed",
      "@type": ["beckn:SettlementCycleStatus", "schema:Enumeration"],
      "schema:name": "FAILED",
      "schema:identifier": "FAILED"
    },
    {
      "@id": "beckn:BillingCycleStatus",
      "@type": "schema:Enumeration",
      "schema:name": "Billing Cycle Status",
      "rdfs:comment": "Enumeration of billing cycle payment statuses.",
      "schema:hasEnumerationMember": [
        { "@id": "beckn:BillingCycleStatusPending" },
        { "@id": "beckn:BillingCycleStatusPaid" },
        { "@id": "beckn:BillingCycleStatusOverdue" }
      ]
    },
    {
      "@id": "beckn:BillingCycleStatusPending",
      "@type": ["beckn:BillingCycleStatus", "schema:Enumeration"],
      "schema:name": "PENDING",
      "schema:identifier": "PENDING"
    },
    {
      "@id": "beckn:BillingCycleStatusPaid",
      "@type": ["beckn:BillingCycleStatus", "schema:Enumeration"],
      "schema:name": "PAID",
      "schema:identifier": "PAID"
