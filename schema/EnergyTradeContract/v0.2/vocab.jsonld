{
  "@context": {
    "@version": 1.1,
    "beckn": "https://raw.githubusercontent.com/beckn/protocol-specifications-new/refs/heads/draft/schema/EnergyTradeContract/v0.2/#",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "beckn:EnergyTradeContract",
      "@type": "rdfs:Class",
      "rdfs:label": "Energy Trade Contract",
      "rdfs:comment": "Represents the commercial agreement for an energy transaction, extending the core Order entity."
    },
    {
      "@id": "beckn:contractStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Contract status",
      "rdfs:comment": "Contract lifecycle status (PENDING, ACTIVE, COMPLETED, TERMINATED).",
      "rdfs:domain": "beckn:EnergyTradeContract",
      "schema:rangeIncludes": "beckn:ContractStatus"
    },
    {
      "@id": "beckn:sourceMeterId",
      "@type": "rdf:Property",
      "rdfs:label": "Source meter ID",
      "rdfs:comment": "Source meter identifier in DER address format (der://meter/{id}).",
      "rdfs:domain": "beckn:EnergyTradeContract",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:targetMeterId",
      "@type": "rdf:Property",
      "rdfs:label": "Target meter ID",
      "rdfs:comment": "Target/consumer meter identifier in DER address format (der://meter/{id}).",
      "rdfs:domain": "beckn:EnergyTradeContract",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:inverterId",
      "@type": "rdf:Property",
