      "schema:rangeIncludes": "schema:StructuredValue"
    },
    {
      "@id": "beckn:remainingQuantity",
      "@type": "rdf:Property",
      "rdfs:label": "Remaining quantity",
      "rdfs:comment": "Remaining tradable quantity in kWh.",
      "rdfs:domain": "beckn:EnergyTradeContract",
      "schema:rangeIncludes": "schema:Number"
