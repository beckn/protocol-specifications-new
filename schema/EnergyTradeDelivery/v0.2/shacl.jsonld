{
  "@context": {
    "@version": 1.1,
    "sh": "http://www.w3.org/ns/shacl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vocab": "./vocab.jsonld/"
  },
  "@graph": [
    {
      "@id": "beckn:EnergyTradeDelivery",
      "@type": "sh:NodeShape",
      "sh:targetClass": "beckn:EnergyTradeDelivery",
      "sh:name": "EnergyTradeDelivery",
      "sh:property": [
        {
          "sh:path": "beckn:deliveryStatus",
          "sh:name": "deliveryStatus",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "PENDING",
            "IN_PROGRESS",
            "COMPLETED",
            "FAILED"
          ]
        },
        {
          "sh:path": "beckn:deliveryMode",
          "sh:name": "deliveryMode",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "EV_CHARGING",
            "BATTERY_SWAP",
            "V2G",
            "GRID_INJECTION"
          ]
        },
        {
          "sh:path": "beckn:deliveredQuantity",
          "sh:name": "deliveredQuantity",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 0
        },
        {
          "sh:path": "schema:startTime",
          "sh:name": "deliveryStartTime",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        },
        {
          "sh:path": "schema:endTime",
          "sh:name": "deliveryEndTime",
          "sh:maxCount": 1,
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        },
        {
          "sh:path": "beckn:meterReadings",
          "sh:name": "meterReadings",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:telemetry",
          "sh:name": "telemetry",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:settlementCycleId",
          "sh:name": "settlementCycleId",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "schema:dateModified",
          "sh:name": "lastUpdated",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        }
      ]
    }
  ]
}
