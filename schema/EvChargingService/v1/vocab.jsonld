{
  "@context": {
    "@version": 1.1,
    "beckn": "./#",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "beckn:ChargingService",
      "@type": "rdfs:Class",
      "rdfs:label": "EV Charging Service",
      "rdfs:comment": "A service that provides electric vehicle charging capabilities"
    },
    {
      "@id": "beckn:ChargingOffer",
      "@type": "rdfs:Class",
      "rdfs:label": "EV Charging Offer",
      "rdfs:comment": "An offer specification for electric vehicle charging"
    },
    {
      "@id": "beckn:ChargingSession",
      "@type": "rdfs:Class",
      "rdfs:label": "EV Charging Session",
      "rdfs:comment": "A charging session for an electric vehicle"
    },
    {
      "@id": "beckn:ChargingPointOperator",
      "@type": "rdfs:Class",
      "rdfs:label": "EV Charging Point Operator",
      "rdfs:comment": "A charging point operator providing electric vehicle charging services"
    },
    {
      "@id": "beckn:connectorType",
      "@type": "rdf:Property",
      "rdfs:label": "Connector type",
      "rdfs:comment": "Physical connector type supported by the EVSE/charger (e.g., CCS2, Type2, CHAdeMO, GB/T).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:maxPowerKW",
      "@type": "rdf:Property",
      "rdfs:label": "Maximum power (kW)",
      "rdfs:comment": "Maximum charging power supported by the EVSE in kilowatts.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:minPowerKW",
      "@type": "rdf:Property",
      "rdfs:label": "Minimum power (kW)",
      "rdfs:comment": "Minimum controllable charging power in kilowatts.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:socketCount",
      "@type": "rdf:Property",
      "rdfs:label": "Socket count",
      "rdfs:comment": "Number of simultaneously usable sockets/connectors.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Integer"
    },
    {
      "@id": "beckn:reservationSupported",
      "@type": "rdf:Property",
      "rdfs:label": "Reservation supported",
      "rdfs:comment": "Whether advance reservations are supported for this connector/site.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Boolean"
    },
    {
      "@id": "beckn:acceptedPaymentMethod",
      "@type": "rdf:Property",
      "rdfs:label": "Accepted payment method",
      "rdfs:comment": "Accepted payment methods using schema.org payment method types (e.g., schema:UPI, schema:CreditCard, schema:Wallet).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:serviceLocation",
      "@type": "rdf:Property",
      "rdfs:label": "Service location",
      "rdfs:comment": "Physical location where the charging service is offered; aligns to Beckn Location in payloads.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Place"
    },
    {
      "@id": "beckn:amenityFeature",
      "@type": "rdf:Property",
      "rdfs:label": "Amenity feature",
      "rdfs:comment": "Nearby amenities such as Restaurant, Restroom, Wi-Fi.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:ocppId",
      "@type": "rdf:Property",
      "rdfs:label": "OCPP identifier",
      "rdfs:comment": "Internal identifier used by the CPO/OCPP backend.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:evseId",
      "@type": "rdf:Property",
      "rdfs:label": "EVSE identifier",
      "rdfs:comment": "Public EVSE identifier (e.g., OCPI/Hubject style).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:roamingNetwork",
      "@type": "rdf:Property",
      "rdfs:label": "Roaming network",
      "rdfs:comment": "Roaming federation or network name, if any.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:parkingType",
      "@type": "rdf:Property",
      "rdfs:label": "Parking type",
      "rdfs:comment": "Type of parking facility (e.g., Mall, Office, FuelStation).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:connectorId",
      "@type": "rdf:Property",
      "rdfs:label": "Connector ID",
      "rdfs:comment": "Provider-specific connector or socket identifier.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:powerType",
      "@type": "rdf:Property",
      "rdfs:label": "Power type",
      "rdfs:comment": "Electrical power type (AC_SINGLE_PHASE, AC_3_PHASE, DC).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:connectorFormat",
      "@type": "rdf:Property",
      "rdfs:label": "Connector format",
      "rdfs:comment": "Physical presentation of the connector (SOCKET or CABLE).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:chargingSpeed",
      "@type": "rdf:Property",
      "rdfs:label": "Charging speed band",
      "rdfs:comment": "Marketing speed band (SLOW, NORMAL, FAST, ULTRAFAST).",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:stationStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Station status",
      "rdfs:comment": "Real-time availability status of the station or connector.",
      "rdfs:domain": "beckn:ChargingService",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:tariffModel",
      "@type": "rdf:Property",
      "rdfs:label": "Tariff model",
      "rdfs:comment": "Charging tariff basis: PER_KWH, PER_MINUTE, SUBSCRIPTION, TIME_OF_DAY.",
      "rdfs:domain": "beckn:ChargingOffer",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:idleFeePolicy",
      "@type": "rdf:Property",
      "rdfs:label": "Idle fee policy",
      "rdfs:comment": "Human-readable policy for post-charge idle fees associated with an offer.",
      "rdfs:domain": "beckn:ChargingOffer",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:buyerFinderFee",
      "@type": "rdf:Property",
      "rdfs:label": "Buyer finder fee",
      "rdfs:comment": "Commission payable (percentage or absolute) associated with an offer/order.",
      "rdfs:domain": ["beckn:ChargingOffer", "beckn:ChargingSession"],
      "schema:rangeIncludes": "schema:PriceSpecification"
    },
    {
      "@id": "beckn:feeType",
      "@type": "rdf:Property",
      "rdfs:label": "Fee type",
      "rdfs:comment": "Fee type (PERCENTAGE or AMOUNT) for buyer finder fee.",
      "rdfs:domain": ["beckn:ChargingOffer", "beckn:ChargingSession"],
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:feeValue",
      "@type": "rdf:Property",
      "rdfs:label": "Fee value",
      "rdfs:comment": "Numeric fee value (interpreted per fee type).",
      "rdfs:domain": ["beckn:ChargingOffer", "beckn:ChargingSession"],
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:sessionStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Session status",
      "rdfs:comment": "High-level charging session state (PENDING, ACTIVE, COMPLETED, INTERRUPTED).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:authorizationMode",
      "@type": "rdf:Property",
      "rdfs:label": "Authorization mode",
      "rdfs:comment": "Mode used to authorize/start the session (APP_QR, RFID, OTP).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:authorizationOtpHint",
      "@type": "rdf:Property",
      "rdfs:label": "Authorization OTP hint",
      "rdfs:comment": "Optional OTP or hint presented to user for kiosk entry.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:meteredEnergyKWh",
      "@type": "rdf:Property",
      "rdfs:label": "Metered energy (kWh)",
      "rdfs:comment": "Energy dispensed during a charging session, in kilowatt-hours.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:meteredDurationMinutes",
      "@type": "rdf:Property",
      "rdfs:label": "Metered duration (minutes)",
      "rdfs:comment": "Duration of the charging session, in minutes.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:chargingPeriods",
      "@type": "rdf:Property",
      "rdfs:label": "Charging periods",
      "rdfs:comment": "List of charging intervals with telemetry dimensions (OCPI-style).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:ItemList"
    },
    {
      "@id": "beckn:dimensionType",
      "@type": "rdf:Property",
      "rdfs:label": "Dimension type",
      "rdfs:comment": "Type of telemetry dimension (ENERGY, POWER, CURRENT, VOLTAGE, STATE_OF_CHARGE).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:volume",
      "@type": "rdf:Property",
      "rdfs:label": "Dimension value",
      "rdfs:comment": "Numeric value for the dimension (units depend on the type).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:totalCost",
      "@type": "rdf:Property",
      "rdfs:label": "Total cost",
      "rdfs:comment": "Total session cost as a price specification.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:PriceSpecification"
    },
    {
      "@id": "beckn:exclVat",
      "@type": "rdf:Property",
      "rdfs:label": "Excl. VAT amount",
      "rdfs:comment": "Total amount excluding VAT.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:inclVat",
      "@type": "rdf:Property",
      "rdfs:label": "Incl. VAT amount",
      "rdfs:comment": "Total amount including VAT.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "beckn:reservationId",
      "@type": "rdf:Property",
      "rdfs:label": "Reservation ID",
      "rdfs:comment": "Server-assigned reservation identifier for a scheduled session.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:gracePeriodMinutes",
      "@type": "rdf:Property",
      "rdfs:label": "Grace period (minutes)",
      "rdfs:comment": "Minutes allowed after scheduled start before releasing the slot.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Integer"
    },
    {
      "@id": "beckn:operatorCode",
      "@type": "rdf:Property",
      "rdfs:label": "Operator code",
      "rdfs:comment": "Short operator code used in roaming/indexes.",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:gstNumber",
      "@type": "rdf:Property",
      "rdfs:label": "GST Number",
      "rdfs:comment": "Tax identifier (India).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:cin",
      "@type": "rdf:Property",
      "rdfs:label": "CIN",
      "rdfs:comment": "Corporate Identification Number (India).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:msme",
      "@type": "rdf:Property",
      "rdfs:label": "MSME ID",
      "rdfs:comment": "MSME registration identifier (India).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:vehicleMake",
      "@type": "rdf:Property",
      "rdfs:label": "Vehicle make",
      "rdfs:comment": "Vehicle make/brand recorded for the session.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": ["schema:Brand", "schema:Text"]
    },
    {
      "@id": "beckn:vehicleModel",
      "@type": "rdf:Property",
      "rdfs:label": "Vehicle model",
      "rdfs:comment": "Vehicle model recorded for the session.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:trackingId",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking ID",
      "rdfs:comment": "Provider tracking identifier for this session.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:trackingUrl",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking URL",
      "rdfs:comment": "Live session dashboard URL.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:URL"
    },
    {
      "@id": "beckn:trackingStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking status",
      "rdfs:comment": "Whether live tracking is currently active.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:lastUpdated",
      "@type": "rdf:Property",
      "rdfs:label": "Last updated",
      "rdfs:comment": "Last telemetry/billing update timestamp (UTC).",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:DateTime"
    },
    {
      "@id": "beckn:startDateTime",
      "@type": "rdf:Property",
      "rdfs:label": "Start date time",
      "rdfs:comment": "Interval start time (UTC) for charging periods.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:DateTime"
    },
    {
      "@id": "beckn:dimensions",
      "@type": "rdf:Property",
      "rdfs:label": "Dimensions",
      "rdfs:comment": "Array of telemetry dimensions for charging periods.",
      "rdfs:domain": "beckn:ChargingSession",
      "schema:rangeIncludes": "schema:ItemList"
    },
    {
      "@id": "beckn:operatorName",
      "@type": "rdf:Property",
      "rdfs:label": "Operator name",
      "rdfs:comment": "Operating company name (display).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:identifier",
      "@type": "rdf:Property",
      "rdfs:label": "Identifier",
      "rdfs:comment": "Provider identifier (URN/URL/code).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:sameAs",
      "@type": "rdf:Property",
      "rdfs:label": "Same as",
      "rdfs:comment": "Canonical reference (e.g., website, registry entry).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:URL"
    },
    {
      "@id": "beckn:supportEmail",
      "@type": "rdf:Property",
      "rdfs:label": "Support email",
      "rdfs:comment": "Support email (if not provided in core Provider).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:EmailAddress"
    },
    {
      "@id": "beckn:supportPhone",
      "@type": "rdf:Property",
      "rdfs:label": "Support phone",
      "rdfs:comment": "Support phone (if not provided in core Provider).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "beckn:operatorContact",
      "@type": "rdf:Property",
      "rdfs:label": "Operator contact",
      "rdfs:comment": "Free-form contact blob for ops (last resort).",
      "rdfs:domain": "beckn:ChargingPointOperator",
      "schema:rangeIncludes": "schema:ContactPoint"
    }
  ]
}
