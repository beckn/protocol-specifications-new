{
  "@context": {
    "@version": 1.1,
    "sh": "http://www.w3.org/ns/shacl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vocab": "./vocab.jsonld/"
  },
  "@graph": [
    {
      "@id": "beckn:ChargingSession",
      "@type": "sh:NodeShape",
      "sh:targetClass": "beckn:ChargingSession",
      "sh:name": "ChargingSession",
      "sh:property": [
        {
          "sh:path": "beckn:sessionStatus",
          "sh:name": "sessionStatus",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "PENDING",
            "ACTIVE",
            "COMPLETED",
            "INTERRUPTED"
          ]
        },
        {
          "sh:path": "beckn:authorizationMode",
          "sh:name": "authorizationMode",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "APP_QR",
            "RFID",
            "OTP"
          ]
        },
        {
          "sh:path": "beckn:authorizationOtpHint",
          "sh:name": "authorizationOtpHint",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "ocpp:ConnectorEnumType",
          "sh:name": "connectorType",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:in": [
            "cCCS1",
            "cCCS2",
            "cG105",
            "cTesla",
            "cType1",
            "cType2",
            "s309-1P-16A",
            "s309-1P-32A",
            "s309-3P-16A",
            "s309-3P-32A",
            "sBS1361",
            "sCEE-7-7",
            "sType2",
            "sType3",
            "Other1PhMax16A",
            "Other1PhOver16A",
            "Other3Ph",
            "Pan",
            "wInductive",
            "wResonant",
            "Undetermined",
            "Unknown"
          ]
        },
        {
          "sh:path": "beckn:maxPowerKW",
          "sh:name": "maxPowerKW",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 1
        },
        {
          "sh:path": "beckn:meteredEnergyKWh",
          "sh:name": "meteredEnergyKWh",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 0
        },
        {
          "sh:path": "beckn:meteredDurationMinutes",
          "sh:name": "meteredDurationMinutes",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 0
        },
        {
          "sh:path": "beckn:chargingTelemetry",
          "sh:name": "chargingTelemetry",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "beckn:totalCost",
          "sh:name": "totalCost",
          "sh:nodeKind": "sh:IRIOrLiteral"
        },
        {
          "sh:path": "schema:dateModified",
          "sh:name": "lastUpdated",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#dateTime"
        },
        {
          "sh:path": "beckn:reservationId",
          "sh:name": "reservationId",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "beckn:gracePeriodMinutes",
          "sh:name": "gracePeriodMinutes",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": 0
        },
        {
          "sh:path": "beckn:vehicleMake",
          "sh:name": "vehicleMake",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        },
        {
          "sh:path": "beckn:vehicleModel",
          "sh:name": "vehicleModel",
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string"
        }
      ]
    }
  ]
}
